package POO;

public class Personne {
	// Les attributs / proprietés
	private String nom;
	private String prenom;
	private String email;
	private int age;
	
	
	public Personne() {
		super();
	
	}


	@Override
	public String toString() {
		return "Personne [nom=" + nom + ", prenom=" + prenom + ", email=" + email + ", age=" + age + "]";
	}
	
}
